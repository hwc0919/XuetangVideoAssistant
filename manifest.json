{
    "name": "Xuetang Video Helper",
    "version": "1.0",
    "description": "Help you switch part in fullscreen state.",
    "permissions": [
        "storage",
        "declarativeContent",
        "activeTab"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/head16.png",
            "32": "images/head32.png",
            "48": "images/head48.png",
            "128": "images/head128.png"
        }
    },
    "icons": {
        "16": "images/head16.png",
        "32": "images/head32.png",
        "48": "images/head48.png",
        "128": "images/head128.png"
    },
    "manifest_version": 2
}